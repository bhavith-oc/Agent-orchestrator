# Server
HOST=0.0.0.0
PORT=8000
SECRET_KEY=aether-orchestrator-secret-key-change-in-production

# Database
DATABASE_URL=sqlite+aiosqlite:///./aether.db

# LLM - OpenRouter (required)
OPENROUTER_API_KEY=your-openrouter-api-key-here
OPENROUTER_BASE_URL=https://openrouter.ai/api/v1

# Jason Config
JASON_MODEL=openai/gpt-4o
JASON_TEMPERATURE=0.3
JASON_MAX_TOKENS=4096

# Sub-Agent Config
SUB_AGENT_MODEL=openai/gpt-4o-mini
SUB_AGENT_TEMPERATURE=0.2
SUB_AGENT_MAX_TOKENS=8192
SUB_AGENT_MAX_RETRIES=3

# Git (set these to your target repo)
REPO_PATH=
WORKTREE_BASE_PATH=

# Remote Jason (OpenClaw) — set to auto-connect on startup
REMOTE_JASON_URL=
REMOTE_JASON_TOKEN=
REMOTE_JASON_SESSION=agent:main:main

# JWT
ACCESS_TOKEN_EXPIRE_MINUTES=1440
ALGORITHM=HS256

# Google OAuth (optional)
GOOGLE_CLIENT_ID=

# CORS — comma-separated origins for VPS (leave empty for localhost defaults)
# Example: CORS_ORIGINS=http://YOUR_VPS_IP:5173,http://YOUR_VPS_IP:3000
CORS_ORIGINS=
