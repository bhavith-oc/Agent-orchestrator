{
  "agents": {
    "defaults": {
      "workspace": "~/.openclaw/workspace",
      "model": {
        "primary": "openrouter/anthropic/claude-sonnet-4"
      },
      "maxConcurrent": 4,
      "subagents": {
        "maxConcurrent": 8,
        "allowAgents": ["*"]
      },
      "compaction": {
        "mode": "safeguard"
      }
    },
    "list": [
      {
        "id": "main",
        "name": "Jason",
        "default": true,
        "workspace": "~/.openclaw/workspace",
        "subagents": {
          "allowAgents": ["*"]
        }
      }
    ]
  },
  "gateway": {
    "mode": "local",
    "auth": {
      "token": "${OPENCLAW_GATEWAY_TOKEN}"
    }
  },
  "commands": {
    "native": "auto",
    "nativeSkills": "auto"
  }
}
